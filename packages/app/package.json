{
  "name": "@sequenced_app/app",
  "private": true,
  "version": "1.0.9",
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:full": "npm run build && npm run sync",
    "sync": "npx cap sync",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "host": "vite --host",
    "open:ios": "npx cap open ios",
    "open:ios:full": "npm run build:full && npm run open:ios",
    "open:android": "npx cap open android",
    "open:android:full": "npm run build:full && npm run open:android",
    "run:ios": "npx cap run ios",
    "run:android": "npx cap run android",
    "build:ios": "npm run build:full && npm run run:ios",
    "build:android": "npm run build:full && npm run run:android",
    "resources": "capacitor-resources -p android,ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/local-notifications": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.3",
    "@tanstack/react-query": "^5.40.1",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.1",
    "vite-plugin-pwa": "^1.0.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
  }
}
