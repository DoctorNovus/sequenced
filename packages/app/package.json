{
  "name": "@sequenced_app/app",
  "type": "module",
  "private": true,
  "version": "1.0.9",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:full": "npm run build && npm run sync",
    "sync": "npx cap sync",
    "host": "vite --host",
    "open:ios": "npx cap open ios",
    "open:ios:full": "npm run build:full && npm run open:ios",
    "open:android": "npx cap open android",
    "open:android:full": "npm run build:full && npm run open:android",
    "run:ios": "npx cap run ios",
    "run:android": "npx cap run android",
    "build:ios": "npm run build:full && npm run run:ios",
    "build:android": "npm run build:full && npm run run:android",
    "resources": "capacitor-resources -p android,ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/local-notifications": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@eslint/js": "^9.39.2",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query": "^5.90.16",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "nanoid": "^5.0.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.11.0",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.0.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  }
}
